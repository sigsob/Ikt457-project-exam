    "\n",
    "        while stack:\n",
    "            cq, cr = stack.pop()\n",
    "            for nq, nr in get_valid_neighbors(cq, cr, dim):\n",
    "                if (nq, nr) not in visited:\n",
    "                    neighbor_value = X_data[graph_id][nq][nr]\n",
    "                    neighbor_symbol = value_to_symbol[neighbor_value]\n",
    "                    if neighbor_symbol == symbol:\n",
    "                        visited.add((nq, nr))\n",
    "                        stack.append((nq, nr))\n",
    "                        size += 1\n",
    "        return size\n",
    "\n",
    "    # Check edge proximity\n",
    "    def check_edge_proximity(q, r):\n",
    "        properties = []\n",
    "        if q == 0: properties.append('near_top_edge')\n",
    "        if q == dim - 1: properties.append('near_bottom_edge')\n",
    "        if r == 0: properties.append('near_left_edge')\n",
    "        if r == dim - 1: properties.append('near_right_edge')\n",
    "        return properties\n",
    "\n",
    "    for graph_id in tqdm(range(X_data.shape[0]), desc=\"Creating testing nodes and adding features\"):\n",
    "        visited = set()  # For cluster size calculation\n",
    "        for q in range(dim):\n",
    "            for r in range(dim):\n",
    "                node_id = q * dim + r\n",
    "                value = X_data[graph_id][q][r]\n",
    "                cell_symbol = value_to_symbol[value]\n",
    "\n",
    "                # Neighbor counts\n",
    "                valid_neighbors = get_valid_neighbors(q, r, dim)\n",
    "                neighbor_counts = {symbol: 0 for symbol in cell_symbols}\n",
    "                for nq, nr in valid_neighbors:\n",
    "                    neighbor_value = X_data[graph_id][nq][nr]\n",
    "                    neighbor_symbol = value_to_symbol[neighbor_value]\n",
    "                    neighbor_counts[neighbor_symbol] += 1\n",
    "\n",
    "                # Calculate cluster size (only unvisited nodes)\n",
    "                if (q, r) not in visited:\n",
    "                    cluster_size = calculate_cluster_size(q, r, cell_symbol, graph_id, visited)\n",
    "                else:\n",
    "                    cluster_size = 0\n",
    "\n",
    "                # Check diagonal pairing\n",
    "                diagonal_pair = False\n",
    "                for nq, nr in get_diagonal_neighbors(q, r, dim):\n",
    "                    if 0 <= nq < dim and 0 <= nr < dim:\n",
    "                        neighbor_value = X_data[graph_id][nq][nr]\n",
    "                        if value_to_symbol[neighbor_value] == cell_symbol:\n",
    "                            diagonal_pair = True\n",
    "\n",
    "                # Check edge proximity\n",
    "                edge_properties = check_edge_proximity(q, r)\n",
    "\n",
    "                graphs_test.add_graph_node(\n",
    "                    graph_id, node_id, number_of_graph_node_edges=len(valid_neighbors)\n",
    "                )\n",
    "\n",
    "                # Add basic properties\n",
    "                graphs_test.add_graph_node_property(graph_id, node_id, cell_symbol)\n",

    "from GraphTsetlinMachine.graphs import Graphs\n",
    "import numpy as np\n",
    "from GraphTsetlinMachine.tm import MultiClassGraphTsetlinMachine\n",
    "from time import time\n",
    "import argparse\n",
    "import os\n",
    "import pandas as pd\n",
    "from collections import deque\n",
    "from sklearn.model_selection import train_test_split\n",
    "from tqdm import tqdm\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def load_data(input_file, sample_fraction=1):\n",
    "    X_data = []\n",
    "    Y_data = []\n",
    "\n",
    "    # Read the CSV file, skipping any headers if present\n",
    "    data = pd.read_csv(input_file, header=0, dtype=str)  # Read everything as strings for now\n",
    "\n",
    "    # Sample a fraction of the data\n",
    "    data_sampled = data.sample(frac=sample_fraction, random_state=42)\n",
    "\n",
    "    # Use tqdm to show progress over the rows in the sampled dataset\n",
    "    for _, row in tqdm(data_sampled.iterrows(), total=len(data_sampled), desc=\"Processing Rows\"):\n",
    "        try:\n",
    "            # Extract the first 49 values\n",
    "            board = row.iloc[:bord_size*bord_size].values.astype(int).reshape(bord_size, bord_size)\n",
    "            # The 50th value is the label (winner)\n",
    "            label_raw = int(row.iloc[bord_size*bord_size])\n",
    "            # Map labels to 0 or 1\n",
    "            if label_raw == 1:\n",
    "                label = 1  # Red wins\n",
    "            elif label_raw == -1:\n",
    "                label = 0  # Blue wins\n",
    "            else:\n",
    "                print(f\"Skipping row due to invalid label: {label_raw}\")\n",
    "                continue\n",
    "        except ValueError:\n",
    "            print(f\"Skipping row due to invalid data: {row}\")\n",
    "            continue\n",
    "\n",
    "        # Append the board and label to the respective lists\n",
    "        X_data.append(board)\n",
    "        Y_data.append(label)\n",
    "\n",
    "    return np.array(X_data), np.array(Y_data, dtype=np.int32)\n",
    "\n",
    "bord_size = 5\n",
    "\n",
    "if bord_size == 5:\n",
    "    X_data, Y_data = load_data(r\"/home/sigurds/GraphTsetlinMachine-master/examples/game_results_5x5_random_ja.csv\")\n",
    "elif bord_size == 7:\n",
    "    X_data, Y_data = load_data(r\"/home/sigurds/GraphTsetlinMachine-master/examples/game_results_7x7_random_ja.csv\")\n",
    "elif bord_size == 9:\n",
    "    X_data, Y_data = load_data(r\"/home/sigurds/GraphTsetlinMachine-master/examples/game_results_9x9_random_ja.csv\")\n",
    "elif bord_size == 15: \n",
    "    X_data, Y_data = load_data(r\"/home/sigurds/GraphTsetlinMachine-master/examples/game_results_15x15_random_2.csv\")\n",
    "\n",
    "\n",
    "    \n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# Split the data into training and testing sets\n",
    "from sklearn.model_selection import train_test_split\n",
    "\n",
    "# Adjust test_size and random_state as needed\n",
    "X_train, X_test, Y_train, Y_test = train_test_split(X_data, Y_data, test_size=0.2, random_state=42)\n",
    "\n"

    "dim = bord_size  # Hexboard dimension is 7x7\n",
    "number_of_nodes = dim * dim\n",
    "\n",
    "\n",
    "def default_args(**kwargs):\n",
    "    parser = argparse.ArgumentParser()\n",
    "    parser.add_argument(\"--epochs\", default=225, type=int)\n",
    "    parser.add_argument(\"--number-of-clauses\", default=20000, type=int)\n",
    "    parser.add_argument(\"--T\", default=20000/1.6, type=int)\n",
    "    parser.add_argument(\"--s\", default=2, type=float)\n",
    "    parser.add_argument(\"--depth\", default=1, type=int)\n",
    "    parser.add_argument(\"--hypervector-size\", default=32, type=int)\n",
    "    parser.add_argument(\"--hypervector-bits\", default=2, type=int)\n",
    "    parser.add_argument(\"--message-size\", default=256, type=int)\n",
    "    parser.add_argument(\"--message-bits\", default=2, type=int)\n",
    "    parser.add_argument('--double-hashing', dest='double_hashing', default=True, action='store_true')\n",
    "    parser.add_argument(\"--max-included-literals\", default=32, type=int)\n",
    "\n",
    "    # Use parse_known_args() to ignore unrecognized arguments\n",
    "    args, _ = parser.parse_known_args()\n",
    "    \n",
    "    for key, value in kwargs.items():\n",
    "        if key in args.__dict__:\n",
    "            setattr(args, key, value)\n",
    "    \n",
    "    return args\n",
    "\n",
    "args = default_args()\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# Define cell symbols, row symbols, and column symbols\n",
    "cell_symbols = ['empty', 'blue', 'red']\n",
    "row_symbols = [f'row_{i}' for i in range(dim)]\n",
    "col_symbols = [f'col_{j}' for j in range(dim)]\n",
    "\n",
    "# Generate all possible neighbor symbols with counts (0-6)\n",
    "neighbor_symbols = [f'nbr_{symbol}_{count}' for symbol in cell_symbols for count in range(7)]\n",
    "\n",
    "# Add chain symbols\n",
    "chain_symbols = ['chain_of_3_true', 'chain_of_3_false']\n",
    "\n",
    "# Add diagonal pair symbols\n",
    "diagonal_symbols = ['diagonal_pair_true', 'diagonal_pair_false']\n",
    "\n",
    "# Add edge proximity symbols\n",
    "edge_symbols = ['near_top_edge', 'near_bottom_edge', 'near_left_edge', 'near_right_edge']\n",
    "\n",
    "# Add cluster size symbols (including cluster_size_0)\n",
    "cluster_size_symbols = [f'cluster_size_{i}' for i in range(dim * dim + 1)]\n",
    "\n",
    "# Combine all symbols\n",
    "symbols = (\n",
    "    cell_symbols +\n",
    "    row_symbols +\n",
    "    col_symbols +\n",
    "    neighbor_symbols +\n",
    "    chain_symbols +\n",
    "    diagonal_symbols +\n",
    "    edge_symbols +\n",
    "    cluster_size_symbols\n",
    ")\n",
    "\n",
    "# Map cell values to symbols\n",
    "value_to_symbol = {\n",
    "    0: 'empty',\n",
    "    -1: 'blue',\n",
    "    1: 'red'\n",
    "}\n",
    "\n",
    "\n",
    "def create_graphs_train(X_data):\n",
    "    graphs_train = Graphs(\n",
    "        X_data.shape[0],\n",
    "        symbols=symbols,  # Include all predefined symbols\n",
    "        hypervector_size=args.hypervector_size,\n",
    "        hypervector_bits=args.hypervector_bits,\n",
    "        double_hashing=args.double_hashing\n",
    "    )\n",
    "\n",
    "    number_of_nodes = dim * dim  # Total number of nodes per graph\n",
    "\n",
    "    for graph_id in range(X_data.shape[0]):\n",
    "        graphs_train.set_number_of_graph_nodes(graph_id, number_of_nodes)\n",
    "\n",
    "    graphs_train.prepare_node_configuration()\n",
    "\n",
    "    # Helper function to get valid neighbors\n",
    "    def get_valid_neighbors(q, r, dim):\n",
    "        return [\n",
    "            (nq, nr) for nq, nr in [\n",
    "                (q - 1, r), (q + 1, r), (q, r - 1), (q, r + 1), (q - 1, r + 1), (q + 1, r - 1)\n",
    "            ] if 0 <= nq < dim and 0 <= nr < dim\n",
    "        ]\n",
    "\n",
    "    # Get diagonal neighbors\n",
    "    def get_diagonal_neighbors(q, r, dim):\n",
    "        return [\n",
    "            (q - 1, r + 1), (q + 1, r - 1)\n",
    "        ]\n",
    "\n",
    "    # DFS to calculate cluster size\n",
    "    def calculate_cluster_size(q, r, symbol, graph_id, visited):\n",
    "        stack = [(q, r)]\n",
    "        visited.add((q, r))\n",
    "        size = 1\n",
    "\n",
    "        while stack:\n",
    "            cq, cr = stack.pop()\n",
    "            for nq, nr in get_valid_neighbors(cq, cr, dim):\n",
    "                if (nq, nr) not in visited:\n",
    "                    neighbor_value = X_data[graph_id][nq][nr]\n",
    "                    neighbor_symbol = value_to_symbol[neighbor_value]\n",
    "                    if neighbor_symbol == symbol:\n",
    "                        visited.add((nq, nr))\n",
    "                        stack.append((nq, nr))\n",
    "                        size += 1\n",
    "        return size\n",
    "\n",
    "    # Check edge proximity\n",
    "    def check_edge_proximity(q, r):\n",
    "        properties = []\n",
    "        if q == 0: properties.append('near_top_edge')\n",
    "        if q == dim - 1: properties.append('near_bottom_edge')\n",
    "        if r == 0: properties.append('near_left_edge')\n",
    "        if r == dim - 1: properties.append('near_right_edge')\n",
    "        return properties\n",
    "\n",
    "    for graph_id in tqdm(range(X_data.shape[0]), desc=\"Creating training nodes and adding features\"):\n",
    "        visited = set()  # For cluster size calculation\n",
    "        for q in range(dim):\n",
    "            for r in range(dim):\n",
    "                node_id = q * dim + r\n",
    "                value = X_data[graph_id][q][r]\n",
    "                cell_symbol = value_to_symbol[value]\n",
    "\n",
    "                # Neighbor counts\n",
    "                valid_neighbors = get_valid_neighbors(q, r, dim)\n",
    "                neighbor_counts = {symbol: 0 for symbol in cell_symbols}\n",
    "                for nq, nr in valid_neighbors:\n",
    "                    neighbor_value = X_data[graph_id][nq][nr]\n",
    "                    neighbor_symbol = value_to_symbol[neighbor_value]\n",
    "                    neighbor_counts[neighbor_symbol] += 1\n",
    "\n",
    "                # Calculate cluster size (only unvisited nodes)\n",
    "                if (q, r) not in visited:\n",
    "                    cluster_size = calculate_cluster_size(q, r, cell_symbol, graph_id, visited)\n",
    "                else:\n",
    "                    cluster_size = 0\n",
    "\n",
    "                # Check diagonal pairing\n",
    "                diagonal_pair = False\n",
    "                for nq, nr in get_diagonal_neighbors(q, r, dim):\n",
    "                    if 0 <= nq < dim and 0 <= nr < dim:\n",
    "                        neighbor_value = X_data[graph_id][nq][nr]\n",
    "                        if value_to_symbol[neighbor_value] == cell_symbol:\n",
    "                            diagonal_pair = True\n",
    "\n",
    "                # Check edge proximity\n",
    "                edge_properties = check_edge_proximity(q, r)\n",
    "\n",
    "                graphs_train.add_graph_node(\n",
    "                    graph_id, node_id, number_of_graph_node_edges=len(valid_neighbors)\n",
    "                )\n",
    "\n",
    "                # Add basic properties\n",
    "                graphs_train.add_graph_node_property(graph_id, node_id, cell_symbol)\n",
    "                graphs_train.add_graph_node_property(graph_id, node_id, f'row_{q}')\n",
    "                graphs_train.add_graph_node_property(graph_id, node_id, f'col_{r}')\n",
    "\n",
    "                # Add neighbor counts\n",
    "                for symbol, count in neighbor_counts.items():\n",
    "                    graphs_train.add_graph_node_property(graph_id, node_id, f'nbr_{symbol}_{count}')\n",
    "\n",
    "                # Add cluster size property\n",
    "                graphs_train.add_graph_node_property(graph_id, node_id, f'cluster_size_{cluster_size}')\n",
    "\n",
    "                # Add diagonal pair property\n",
    "                diagonal_symbol = 'diagonal_pair_true' if diagonal_pair else 'diagonal_pair_false'\n",
    "                graphs_train.add_graph_node_property(graph_id, node_id, diagonal_symbol)\n",
    "\n",
    "                # Add edge proximity properties\n",
    "                for edge_property in edge_properties:\n",
    "                    graphs_train.add_graph_node_property(graph_id, node_id, edge_property)\n",
    "\n",
    "    graphs_train.prepare_edge_configuration()\n",
    "\n",
    "    for graph_id in tqdm(range(X_data.shape[0]), desc=\"Adding training edges\"):\n",
    "        for q in range(dim):\n",
    "            for r in range(dim):\n",
    "                node_id = q * dim + r\n",
    "                for nq, nr in get_valid_neighbors(q, r, dim):\n",
    "                    neighbor_node_id = nq * dim + nr\n",
    "                    graphs_train.add_graph_node_edge(graph_id, node_id, neighbor_node_id, \"Neighbor\")\n",
    "\n",
    "    graphs_train.encode()\n",
    "    return graphs_train\n",
    "\n",
    "\n",
    "\n",
    "def create_graphs_test(X_data, graphs_train):\n",
    "    graphs_test = Graphs(\n",
    "        X_data.shape[0],\n",
    "        symbols=symbols,  # Include all predefined symbols\n",
    "        hypervector_size=args.hypervector_size,\n",
    "        hypervector_bits=args.hypervector_bits,\n",
    "        double_hashing=args.double_hashing,\n",
    "        init_with=graphs_train  # Initialize with training graphs\n",
    "    )\n",
    "\n",
    "    number_of_nodes = dim * dim  # Total number of nodes per graph\n",
    "\n",
    "    for graph_id in range(X_data.shape[0]):\n",
    "        graphs_test.set_number_of_graph_nodes(graph_id, number_of_nodes)\n",
    "\n",
    "    graphs_test.prepare_node_configuration()\n",
    "\n",
    "    # Helper function to get valid neighbors\n",
    "    def get_valid_neighbors(q, r, dim):\n",

    "\n",
    "# Create and train the Tsetlin Machine\n",
    "tm = MultiClassGraphTsetlinMachine(\n",
    "    number_of_clauses=7200,\n",
    "    T=7200/1.6,\n",
    "    s=args.s,\n",
    "    depth=1,  \n",
    "    max_included_literals=32,\n",
    "    message_size=256,\n",
    "    message_bits=2,\n",
    ")\n",
    "\n",
    "# Initialize lists to store accuracy values\n",
    "train_accuracies = []\n",
    "test_accuracies = []\n",
    "clause_activations = []\n",
    "\n",
    "feature_importance = {symbol: 0 for symbol in symbols}\n",
    "\n",
    "# Training loop with accuracy recording\n",
    "for i in range(150):\n",
    "    start_training = time()\n",
    "    tm.fit(graphs_train, Y_train, epochs=1, incremental=True)\n",
    "    stop_training = time()\n",
    "\n",
    "    if hasattr(tm, 'clause_log'):\n",
    "        print(f\"Clause Activity for Epoch {i}:\")\n",
    "        for clause_id, activity in enumerate(tm.clause_log):\n",
    "            print(f\"Clause {clause_id}: {activity}\")\n",
    "\n",
    "    train_accuracy = 100 * (tm.predict(graphs_train) == Y_train).mean()\n",
    "    test_accuracy = 100 * (tm.predict(graphs_test) == Y_test).mean()\n",
    "    \n",
    "    train_accuracies.append(train_accuracy)\n",
    "    test_accuracies.append(test_accuracy)\n",
    "    \n",
